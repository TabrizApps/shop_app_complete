<?xml version="1.0" encoding="UTF-8"?>
<project name="shop_app_complete" default="all">
  
  
  <property file="shop_app_complete.properties"/>
  <!-- Uncomment the following property if no tests compilation is needed -->
  <!-- 
  <property name="skip.tests" value="true"/>
   -->
  
  <!-- Compiler options -->
  
  <property name="compiler.debug" value="on"/>
  <property name="compiler.generate.no.warnings" value="off"/>
  <property name="compiler.args" value=""/>
  <property name="compiler.max.memory" value="700m"/>
  <patternset id="ignored.files">
    <exclude name="**/*.hprof/**"/>
    <exclude name="**/*.pyc/**"/>
    <exclude name="**/*.pyo/**"/>
    <exclude name="**/*.rbc/**"/>
    <exclude name="**/*.yarb/**"/>
    <exclude name="**/*~/**"/>
    <exclude name="**/.DS_Store/**"/>
    <exclude name="**/.git/**"/>
    <exclude name="**/.hg/**"/>
    <exclude name="**/.svn/**"/>
    <exclude name="**/CVS/**"/>
    <exclude name="**/__pycache__/**"/>
    <exclude name="**/_svn/**"/>
    <exclude name="**/vssver.scc/**"/>
    <exclude name="**/vssver2.scc/**"/>
  </patternset>
  <patternset id="library.patterns">
    <include name="*.egg"/>
    <include name="*.jar"/>
    <include name="*.ear"/>
    <include name="*.klib"/>
    <include name="*.swc"/>
    <include name="*.war"/>
    <include name="*.zip"/>
    <include name="*.ane"/>
  </patternset>
  <patternset id="compiler.resources">
    <exclude name="**/?*.java"/>
    <exclude name="**/?*.form"/>
    <exclude name="**/?*.class"/>
    <exclude name="**/?*.groovy"/>
    <exclude name="**/?*.scala"/>
    <exclude name="**/?*.flex"/>
    <exclude name="**/?*.kt"/>
    <exclude name="**/?*.clj"/>
    <exclude name="**/?*.aj"/>
  </patternset>
  
  <property name="project.jdk.home" value=""/>
  <property name="project.jdk.bin" value=""/>
  <property name="project.jdk.classpath" value=""/>
  
  
  <!-- Project Libraries -->
  
  <path id="library.dart_packages.classpath">
    <pathelement location="${basedir}/../../../android/flutter/.pub-cache/hosted/pub.dartlang.org/async-2.3.0/lib"/>
    <pathelement location="${basedir}/../../../android/flutter/.pub-cache/hosted/pub.dartlang.org/boolean_selector-1.0.5/lib"/>
    <pathelement location="${basedir}/../../../android/flutter/.pub-cache/hosted/pub.dartlang.org/charcode-1.1.2/lib"/>
    <pathelement location="${basedir}/../../../android/flutter/.pub-cache/hosted/pub.dartlang.org/collection-1.14.11/lib"/>
    <pathelement location="${basedir}/../../../android/flutter/.pub-cache/hosted/pub.dartlang.org/cupertino_icons-0.1.2/lib"/>
    <pathelement location="${basedir}/../../../android/flutter/.pub-cache/hosted/pub.dartlang.org/http-0.12.0+2/lib"/>
    <pathelement location="${basedir}/../../../android/flutter/.pub-cache/hosted/pub.dartlang.org/http_parser-3.1.3/lib"/>
    <pathelement location="${basedir}/../../../android/flutter/.pub-cache/hosted/pub.dartlang.org/intl-0.16.0/lib"/>
    <pathelement location="${basedir}/../../../android/flutter/.pub-cache/hosted/pub.dartlang.org/matcher-0.12.5/lib"/>
    <pathelement location="${basedir}/../../../android/flutter/.pub-cache/hosted/pub.dartlang.org/meta-1.1.7/lib"/>
    <pathelement location="${basedir}/../../../android/flutter/.pub-cache/hosted/pub.dartlang.org/path-1.6.4/lib"/>
    <pathelement location="${basedir}/../../../android/flutter/.pub-cache/hosted/pub.dartlang.org/pedantic-1.8.0+1/lib"/>
    <pathelement location="${basedir}/../../../android/flutter/.pub-cache/hosted/pub.dartlang.org/provider-3.1.0+1/lib"/>
    <pathelement location="${basedir}/../../../android/flutter/.pub-cache/hosted/pub.dartlang.org/quiver-2.0.5/lib"/>
    <pathelement location="${basedir}/../../../android/flutter/.pub-cache/hosted/pub.dartlang.org/source_span-1.5.5/lib"/>
    <pathelement location="${basedir}/../../../android/flutter/.pub-cache/hosted/pub.dartlang.org/stack_trace-1.9.3/lib"/>
    <pathelement location="${basedir}/../../../android/flutter/.pub-cache/hosted/pub.dartlang.org/stream_channel-2.0.0/lib"/>
    <pathelement location="${basedir}/../../../android/flutter/.pub-cache/hosted/pub.dartlang.org/string_scanner-1.0.5/lib"/>
    <pathelement location="${basedir}/../../../android/flutter/.pub-cache/hosted/pub.dartlang.org/term_glyph-1.1.0/lib"/>
    <pathelement location="${basedir}/../../../android/flutter/.pub-cache/hosted/pub.dartlang.org/test_api-0.2.5/lib"/>
    <pathelement location="${basedir}/../../../android/flutter/.pub-cache/hosted/pub.dartlang.org/typed_data-1.1.6/lib"/>
    <pathelement location="${basedir}/../../../android/flutter/.pub-cache/hosted/pub.dartlang.org/vector_math-2.0.8/lib"/>
    <pathelement location="${basedir}/../../../android/flutter/bin/cache/pkg/sky_engine/lib"/>
    <pathelement location="${basedir}/../../../android/flutter/packages/flutter/lib"/>
    <pathelement location="${basedir}/../../../android/flutter/packages/flutter_test/lib"/>
  </path>
  
  <path id="library.dart_sdk.classpath">
    <pathelement location="${basedir}/../../../android/flutter/bin/cache/dart-sdk/lib/async"/>
    <pathelement location="${basedir}/../../../android/flutter/bin/cache/dart-sdk/lib/collection"/>
    <pathelement location="${basedir}/../../../android/flutter/bin/cache/dart-sdk/lib/convert"/>
    <pathelement location="${basedir}/../../../android/flutter/bin/cache/dart-sdk/lib/core"/>
    <pathelement location="${basedir}/../../../android/flutter/bin/cache/dart-sdk/lib/developer"/>
    <pathelement location="${basedir}/../../../android/flutter/bin/cache/dart-sdk/lib/html"/>
    <pathelement location="${basedir}/../../../android/flutter/bin/cache/dart-sdk/lib/io"/>
    <pathelement location="${basedir}/../../../android/flutter/bin/cache/dart-sdk/lib/isolate"/>
    <pathelement location="${basedir}/../../../android/flutter/bin/cache/dart-sdk/lib/math"/>
    <pathelement location="${basedir}/../../../android/flutter/bin/cache/dart-sdk/lib/mirrors"/>
    <pathelement location="${basedir}/../../../android/flutter/bin/cache/dart-sdk/lib/typed_data"/>
  </path>
  
  <path id="library.flutter_plugins.classpath"/>
  <!-- Register Custom Compiler Taskdefs -->
  <property name="javac2.home" value="${idea.home}/lib"/>
  <path id="javac2.classpath">
    <fileset dir="${javac2.home}">
      <include name="javac2.jar"/>
      <include name="jdom.jar"/>
      <include name="asm-all*.jar"/>
      <include name="forms-*.jar"/>
    </fileset>
  </path>
  <target name="register.custom.compilers">
    <taskdef name="javac2" classname="com.intellij.ant.Javac2" classpathref="javac2.classpath"/>
    <taskdef name="instrumentIdeaExtensions" classname="com.intellij.ant.InstrumentIdeaExtensions" classpathref="javac2.classpath"/>
  </target>
  
  <!-- Modules -->
  
  
  <!-- Module shop_app_complete -->
  
  <dirname property="module.shop_app_complete.basedir" file="${ant.file}"/>
  
  
  <property name="module.jdk.home.shop_app_complete" value=""/>
  <property name="module.jdk.bin.shop_app_complete" value=""/>
  <property name="module.jdk.classpath.shop_app_complete" value=""/>
  
  <property name="compiler.args.shop_app_complete" value="-encoding UTF-8 -source 12 -target 12 ${compiler.args}"/>
  
  <property name="shop_app_complete.output.dir" value="${module.shop_app_complete.basedir}/out/production/shop_app_complete"/>
  <property name="shop_app_complete.testoutput.dir" value="${module.shop_app_complete.basedir}/out/test/shop_app_complete"/>
  
  <path id="shop_app_complete.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="shop_app_complete.module.production.classpath">
    <path refid="library.dart_sdk.classpath"/>
    <path refid="library.dart_packages.classpath"/>
    <path refid="library.flutter_plugins.classpath"/>
  </path>
  
  <path id="shop_app_complete.runtime.production.module.classpath">
    <pathelement location="${shop_app_complete.output.dir}"/>
    <path refid="library.dart_sdk.classpath"/>
    <path refid="library.dart_packages.classpath"/>
    <path refid="library.flutter_plugins.classpath"/>
  </path>
  
  <path id="shop_app_complete.module.classpath">
    <pathelement location="${shop_app_complete.output.dir}"/>
    <path refid="library.dart_sdk.classpath"/>
    <path refid="library.dart_packages.classpath"/>
    <path refid="library.flutter_plugins.classpath"/>
  </path>
  
  <path id="shop_app_complete.runtime.module.classpath">
    <pathelement location="${shop_app_complete.testoutput.dir}"/>
    <pathelement location="${shop_app_complete.output.dir}"/>
    <path refid="library.dart_sdk.classpath"/>
    <path refid="library.dart_packages.classpath"/>
    <path refid="library.flutter_plugins.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.shop_app_complete">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.shop_app_complete">
    <patternset refid="excluded.from.module.shop_app_complete"/>
  </patternset>
  
  
  <target name="compile.module.shop_app_complete" depends="compile.module.shop_app_complete.production,compile.module.shop_app_complete.tests" description="Compile module shop_app_complete"/>
  
  <target name="compile.module.shop_app_complete.production" depends="register.custom.compilers" description="Compile module shop_app_complete; production classes"/>
  
  <target name="compile.module.shop_app_complete.tests" depends="register.custom.compilers,compile.module.shop_app_complete.production" description="compile module shop_app_complete; test classes" unless="skip.tests"/>
  
  <target name="clean.module.shop_app_complete" description="cleanup module">
    <delete dir="${shop_app_complete.output.dir}"/>
    <delete dir="${shop_app_complete.testoutput.dir}"/>
  </target>
  
  <target name="init" description="Build initialization">
    <!-- Perform any build initialization in this target -->
  </target>
  
  <target name="clean" depends="clean.module.shop_app_complete" description="cleanup all"/>
  
  <target name="build.modules" depends="init, clean, compile.module.shop_app_complete" description="build all modules"/>
  
  <target name="all" depends="build.modules" description="build all"/>
</project>